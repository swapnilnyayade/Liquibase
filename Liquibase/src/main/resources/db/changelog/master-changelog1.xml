<databaseChangeLog 
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
 
   <property name="table_name1" value="Student" /> 
   
   
   <changeSet id="1" author="ramesh">
   
      <createTable tableName="${table_name1}">
         <column name="StudentId" type="int" autoIncrement="true">
            <constraints primaryKey="true" nullable="false" />
         </column>
         <column name="StudentName" type="varchar(255)">
            <constraints nullable="true" />
         </column>
      </createTable>
      
   </changeSet>
   
   <changeSet id="2" author="ajit">
   
       <insert tableName="${table_name1}">
            <column name="studentName" value="Tom"/>
       </insert>
       
   </changeSet>
   
  <changeSet id="3" author="deoram">
       
       <update tableName="${table_name1}">
            <column name="studentName" value="Jerry"/>
            <where>studentName = 'Tom'</where>
        </update>
   </changeSet>  
   
   <changeSet id="4" author="swap">
       
            <dropTable tableName="${table_name1}"/>
   </changeSet>  
   
</databaseChangeLog>